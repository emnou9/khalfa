pipeline {
   agent any

    stages {
        stage('Chekout') {
  echo 'Building..'        
	}  
	
        stage('Build') {
            steps {
	    bat label: '', script: 'mvn install'
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
	    bat label: '', script: '''mvn test
'''
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
